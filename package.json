{
  "name": "metalsmith-webpack",
  "version": "0.0.1",
  "description": "A webpack plugin for Metalsmith",
  "keywords": [
    "metalsmith",
    "javascript",
    "webpack"
  ],
  "author": "Christopher Cliff <ctcliff@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/christophercliff/metalsmith-webpack"
  },
  "license": "MIT",
  "main": "./lib/",
  "scripts": {
    "test": "./node_modules/.bin/jshint ./lib/ ./test/ --exclude ./test/fixtures/ && ./node_modules/.bin/mocha --reporter spec"
  },
  "dependencies": {
    "enhanced-resolve": "^0.7.3",
    "underscore": "^1.6.0",
    "webpack": "^1.1.2"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.0.1",
    "jshint": "^2.4.4",
    "metalsmith": "^0.5.0",
    "mocha": "^1.18.2"
  },
  "jshintConfig": {
    "eqeqeq": true,
    "immed": true,
    "latedef": "nofunc",
    "quotmark": "single",
    "trailing": true,
    "unused": true,
    "asi": true,
    "boss": true,
    "expr": true,
    "laxbreak": true,
    "laxcomma": true,
    "scripturl": true,
    "sub": true,
    "loopfunc": true
  }
}
